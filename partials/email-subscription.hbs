<form class="gh-form" data-members-form>
    <input class="gh-form-input" id="{{email_field_id}}" name="email" type="email" placeholder="ihre-email@beispiel.de" required data-members-email>
    <button class="gh-button" type="submit" aria-label="Anmelden">
        <span><span>Anmelden</span> {{> "icons/arrow"}}</span>
        {{> "icons/loader"}}
        {{> "icons/checkmark"}}
    </button>
    <p data-members-error></p>
</form>